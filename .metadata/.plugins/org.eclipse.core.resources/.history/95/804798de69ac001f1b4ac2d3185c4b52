package Buoi3;

public class Array {
public static void main(String[] args) {
	int arr[] = {11,12,13,14};
	int a[] = new int[3];
	a[0]= 10;
	a[1]= 20;
	a[2]= 30;
	
	for(int i = 0; i< a.length; i++) {
	System.out.println(a[i]);
}
	
	String b[] = new String[4];
	b[0]= "10";
	b[1]= "20";
	b[2]= "30";
	b[3]= "40";
	
	int n1 = Integer.valueOf(b[0]).intValue();// chuyá»ƒn String snag int
	int n2 = Integer.valueOf(b[1]).intValue();
	
	System.out.println(n1+n2);
}
}

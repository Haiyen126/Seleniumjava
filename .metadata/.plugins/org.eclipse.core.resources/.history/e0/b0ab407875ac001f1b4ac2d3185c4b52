package Buoi3;

import java.util.Iterator;

public class function {
static void Inputdata(int arr[], int a) {
	for(int i = 0; i<arr.length; i++) {
		arr[i]= a;
	}
}
static void Printdata(int arr[]) {
	for(int i = 0; i<arr.length; i++) {
		System.out.println(arr[i]);
	}
}
	
	public static void main(String[] args) {
		int arr1[] = new int[10];
		
		Inputdata(arr1, 4);
		Printdata(arr1);

	}

}
